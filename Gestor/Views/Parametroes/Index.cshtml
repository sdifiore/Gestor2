@model IEnumerable<Gestor.Models.Parametro>

@{
    ViewBag.Title = "Índice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Índice</h2>

<p>
    @Html.ActionLink("Novo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dolar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropFitasFioGaxeta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropSucatas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropGrafitado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dolar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PropFitasFioGaxeta)%
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PropSucatas)%
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PropGrafitado)%
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
