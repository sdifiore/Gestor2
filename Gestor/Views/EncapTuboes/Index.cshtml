@model IEnumerable<Gestor.Models.EncapTubo>

@{
    ViewBag.Title = "Índice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Índice</h2>

<p>
    @Html.ActionLink("Criar novo encapamento de tubos", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Produto.Apelido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Unidade.Apelido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DextRevest)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.DintRevest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResinaBase)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aditivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DenRev)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PesoRevest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VelRevest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PctCarga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resina)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Master)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Produto.Apelido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Unidade.Apelido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DextRevest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DintRevest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResinaBase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Aditivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DenRev)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PesoRevest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VelRevest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PctCarga)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resina)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Master)
        </td>
        <td>
            @Html.ActionLink("Alterar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.Id })
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
